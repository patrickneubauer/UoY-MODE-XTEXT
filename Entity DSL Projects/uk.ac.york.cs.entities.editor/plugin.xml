<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated entities -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="uk.ac.york.cs.entities.presentation.EntitiesModelWizardID"
            name="%_UI_EntitiesModelWizard_label"
            class="uk.ac.york.cs.entities.presentation.EntitiesModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/EntitiesModelFile.gif">
         <description>%_UI_EntitiesModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated entities -->
      <editor
            id="uk.ac.york.cs.entities.presentation.EntitiesEditorID"
            name="%_UI_EntitiesEditor_label"
            icon="icons/full/obj16/EntitiesModelFile.gif"
            extensions="entities"
            class="uk.ac.york.cs.entities.presentation.EntitiesEditor"
            contributorClass="uk.ac.york.cs.entities.presentation.EntitiesActionBarContributor">
      </editor>
   </extension>

</plugin>
